type User {
  id: ID!
  email: String!
  type: String!
  name: String
  image: String
  username: String
  domain: String
  emailVerified: String
}

type Query {
  available(username: String!): Boolean!
  domainAvailable(domain: String!): Boolean!
  me: User!
}

type Mutation {
  updateUsername(username: String!): User!
}
